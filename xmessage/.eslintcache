 useState([]);\n    \n    useEffect(() => {\n        //No SQL structure of getting back\n        db.collection(\"chats\").onSnapshot((snapshot) =>\n            setChats(\n                snapshot.docs.map((doc) => ({\n                id: doc.id,\n                data: doc.data(),\n            }))\n        ));\n    }, []);\n\n    const addChat = () => {\n\n        const chatName = prompt('Please enter a chat name');\n\n        if (chatName) {\n            db.collection(\"chats\").add({\n                chatName: chatName,\n            });\n        }  \n    }\n    return (\n        <div className=\"sidebar\">\n            <div className=\"sidebar__header\">\n            <Avatar onClick={\n                () => auth().logout } className=\"sidebar__avatar\" src = {user.photo}/>\n            <div className=\"sidebar__input\">\n                <SearchIcon/>\n                <input type=\"text\" placeholder=\"Search\"/>\n            </div>\n\n            <IconButton variant = \"outlined\" className=\"sidebar__inputButton\">\n                <RateReviewOutlinedIcon onClick={addChat}/>\n            </IconButton>\n            \n            </div>\n\n            <div className=\"sidebar__chats\">\n                {/* Clever chat rendering */}\n                {chats.map(({id, data: {chatName} }) => {\n                    <SidebarChat key= {id} id={id} chatName={chatName}/>\n                })}\n        \n            </div>\n        </div>\n    )\n}\n\nexport default Sidebar\n","/home/tiokeng/Desktop/Curent Temp/xMessage/xmessage/src/Xmessage.js",[],"/home/tiokeng/Desktop/Curent Temp/xMessage/xmessage/src/SidebarChat.js",[],"/home/tiokeng/Desktop/Curent Temp/xMessage/xmessage/src/Chat.js",[],"/home/tiokeng/Desktop/Curent Temp/xMessage/xmessage/src/features/userSlice.js",[],"/home/tiokeng/Desktop/Curent Temp/xMessage/xmessage/src/Login.js",[],"/home/tiokeng/Desktop/Curent Temp/xMessage/xmessage/src/firebase.js",[],{"ruleId":"52","replacedBy":"53"},{"ruleId":"54","replacedBy":"55